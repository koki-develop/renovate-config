  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  customManagers: [
    {
      customType: "regex",
      fileMatch: ["^biome\\.json$"],
      matchStrings: ["https://biomejs\\.dev/schemas/(?<currentValue>[^/]+)/schema\\.json"],
      datasourceTemplate: "npm",
      depNameTemplate: "@biomejs/biome",
    },
  ],
}
