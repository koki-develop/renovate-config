{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^mise\\.toml$"],
      "matchStrings": [
        "\"go:(?<depName>[^\"]+)\"\\s*=\\s*\"(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "go"
    },
    {
      "customType": "regex",
      "fileMatch": ["^mise\\.toml$"],
      "matchStrings": [
        "\"npm:(?<depName>[^\"]+)\"\\s*=\\s*\"(?<currentValue>[^\"]+)\""
      ],
      "datasourceTemplate": "npm",
      "versioningTemplate": "npm"
    }
  ]
}

